<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Main Suite" verbose="1" parallel="tests" thread-count="2">
    <parameter name="server" value="http://127.0.0.1:4444/wd/hub"/>

    <test name="iOS Test" enabled="true">
        <parameter name="platformName" value="ios"/>
        <parameter name="deviceName" value="iPhone 8"/>
        <parameter name="platformVersion" value="12.1"/>
        <parameter name="app" value="tryit_stg.ipa"/>
        <classes>
            <class name="ParallelTestRunner"/>
        </classes>
    </test>
    <test name="Android Test" enabled="true">
        <parameter name="platformName" value="android"/>
        <parameter name="deviceName" value="Nexus 5X"/>
        <parameter name="platformVersion" value="8.0.0"/>
        <parameter name="app" value="jp.co.trygroup.tryit.student.ui.staging_v3.1.19.apk"/>
        <classes>
            <class name="ParallelTestRunner"/>
        </classes>
    </test>
</suite>