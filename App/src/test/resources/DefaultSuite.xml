<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Default Test Suite" verbose="1">
    <parameter name="server" value="http://127.0.0.1:4723/wd/hub"/>

    <test name="iOS Test" enabled="false">
        <parameter name="platformName" value="ios"/>
        <parameter name="deviceName" value="iPhone XS Max"/>
        <parameter name="platformVersion" value="12.0"/>
        <classes>
            <class name="TestRunner"/>
        </classes>
    </test>
    <test name="Android Test" enabled="true">
        <parameter name="platformName" value="android"/>
        <parameter name="deviceName" value="Nexus 5"/>
        <parameter name="platformVersion" value="7.0"/>
        <classes>
            <class name="TestRunner"/>
        </classes>
    </test>
</suite>
